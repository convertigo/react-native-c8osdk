{
  "name": "react-native-c8osdk",
  "version": "1.0.5-beta3",
  "description": "Use Convertigo MBaaS from React native apps",
  "main": "src/all.js",
  "typings": "src/all.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/convertigo/react-native-c8osdk"
  },
  "scripts": {
    "prepare":"generate"
    "shipit": "np",
    "generateTS": "tsc -p ./src/",
    "generateFlow": "flowgen ./src/c8o.ts -o ./src/c8o.flow.js && flowgen ./src/c8oLogLevel.ts -o ./src/c8oLogLevel.flow.js && flowgen ./src/c8oSettings.ts -o ./src/c8oSettings.flow.js && flowgen ./src/c8oPromise.ts -o ./src/c8oPromise.flow.js && flowgen ./src/c8oLogger.ts -o ./src/c8oLogger.flow.js",
    "generate": "generateTS && generateFlow && docs",
    "docs": "typedoc --theme markdown --mdEngine github --mdHideSources --module 'commonjs' --target 'ES5' --out ./apidoc/ ./src/"
  },
  "keywords": [
    "react-component",
    "react-native",
    "ios",
    "android",
    "windows",
    "device",
    "events",
    "cocoapod",
    "mbaas",
    "madp"
  ],
  "author": "Charles Grimont <charlesg@convertigo.com> (https://github.com/CharlesGrimont)",
  "contributors": [
    {
      "name": "Nicolas Albert",
      "email": "nicolasa@convertigo.com",
      "url": "https://github.com/nicolas-albert"
    },
    {
      "name": "Olivier Picioto",
      "email": "olivierp@convertigo.com",
      "url": "https://github.com/olivierpiccioto"
    }
  ],
  "license": "Apache 2.0",
  "devDependencies": {
    "flowgen": "^1.2.2",
    "np": "^2.16.0",
    "prettier": "^1.10.2",
    "typedoc": "^0.11.1",
    "typedoc-plugin-markdown": "^1.1.13",
    "typescript": "^2.9.2"
  },
  "dependencies": {
    "rxjs": "^6.2.1",
    "rxjs-compat": "^6.2.1",
    "uuidv4": "^1.0.1"
  }
}
